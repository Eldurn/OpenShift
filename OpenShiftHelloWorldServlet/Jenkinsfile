node {
    stage('Build') {
    	steps {
	         echo 'Building....'
	         echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
	         openshiftScale apiURL: '', authToken: '', depCfg: 'jws-app', namespace: '', replicaCount: '0', verbose: 'false', verifyReplicaCount: 'false', waitTime: ''
	         openshiftBuild apiURL: '', authToken: '', bldCfg: 'jws-app', buildName: '', checkForTriggeredDeployments: 'false', commitID: '', namespace: '', showBuildLogs: 'false', verbose: 'false', waitTime: '', waitUnit: 'sec'
         }
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
    'openshiftDeploy apiURL: '', authToken: '', depCfg: 'jws-app', namespace: '', verbose: 'false', waitTime: '', waitUnit: 'sec'
        echo 'Deploying....'
    }
}